{
  "name": "book-monkey",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-de": "ng serve --configuration=de",
    "build": "ng build",
    "build-prod-gh": "ng build --configuration=production --base-href=https://bodote.github.io/book-monkey/ && ln dist/book-monkey/index.html dist/book-monkey/404.html",
    "build-prod": "ng build --configuration=production ",
    "build-de": "ng build --configuration=production,de",
    "extract-i18n": "ng extract-i18n --format=xmb --output-path=src ",
    "watch": "ng build --watch --configuration development",
    "test-watch": "ng test --watch",
    "test": "ng test --watch=false",
    "test-coverage": "ng test --no-watch --code-coverage",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "deploy": "ng deploy --no-silent --base-href=/book-monkey/",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "stryker:run": "stryker run  --ignorePatterns '**','!src/**/search.reducer.ts'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.0",
    "@angular/common": "^14.0.0",
    "@angular/compiler": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@cypress/schematic": "^1.0.0",
    "@dimaslz/ng-heroicons": "^0.1.5",
    "@ngrx/effects": "^14.3.1",
    "@ngrx/entity": "^14.3.1",
    "@ngrx/router-store": "^14.3.1",
    "@ngrx/store": "^14.3.1",
    "@ngrx/store-devtools": "^14.3.1",
    "angular-date-value-accessor": "^2.0.1",
    "daisyui": "^2.22.0",
    "lodash.isequal": "^4.5.0",
    "rxjs": "~7.5.0",
    "semantic-ui-css": "^2.4.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.3",
    "@angular-eslint/builder": "14.0.0",
    "@angular-eslint/eslint-plugin": "14.0.0",
    "@angular-eslint/eslint-plugin-template": "14.0.0",
    "@angular-eslint/schematics": "14.0.0",
    "@angular-eslint/template-parser": "14.0.0",
    "@angular/cli": "~14.0.3",
    "@angular/compiler-cli": "^14.0.0",
    "@angular/localize": "^14.0.3",
    "@ngrx/eslint-plugin": "^14.3.1",
    "@ngrx/schematics": "^14.3.1",
    "@stryker-mutator/core": "^6.1.2",
    "@stryker-mutator/karma-runner": "^6.1.2",
    "@stryker-mutator/typescript-checker": "^6.1.2",
    "@types/jasmine": "~4.0.0",
    "@types/lodash.isequal": "^4.5.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "angular-cli-ghpages": "^1.0.3",
    "autoprefixer": "^10.4.8",
    "cypress": "^10.6.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-eslint-cli": "^6.0.1",
    "tailwindcss": "^3.1.7",
    "typescript": "~4.7.2"
  }
}
